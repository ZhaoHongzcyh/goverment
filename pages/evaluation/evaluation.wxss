/* pages/evaluation/evaluation.wxss */
page{
  background-color: #F5F5F5;
}

.singlelist{
  width: 660rpx;
  height: 88rpx;
  margin: auto;
  margin-bottom: 40rpx;
  display: flex;
  align-items: center;
  background-color: white;
  border: 2rpx solid #999;
  border-radius: 10rpx;
}

.title{
  flex: 1;
  padding-left: 22rpx;
  font-size: 28rpx;
  color: #333;
}

.arrow{
  width: 20rpx;
  height: 20rpx;
  margin-right: 28rpx;
}

.loaderror{
  position: fixed;
  width: 100%;
  margin-top: 30%;
}
