.page{
  position: fixed;
  width: 100%;
  height: calc(100% - 110rpx);
  overflow: auto;
}

.progress{
  display: flex;
  height: 72rpx;
  font-size: 24rpx;
  background-color: rgba(245,245,245,1);
  align-items: center;
  color: #333;
}

.progress>progress{
  margin-left: 24rpx;
  width: 569rpx;
  height: 20rpx;
  background-color:#D7DEF7;
}

.numinfo{
  margin-left: 40rpx;
}

.noworder{
  color: red;
}

.turnsubject{
  display: flex;
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  border: 2rpx solid #4963C4;
  align-items: center;
}

.primary{
  width: 50%;
  height: 96rpx;
  line-height: 96rpx;
  font-size: 36rpx;
  text-align: center;
  color: #4963C4;
}

.pagedown{
  color: white;
  background-color: #4963C4;
}

.firstpage{
  width: 100%;
  height: 96rpx;
  line-height: 96rpx;
  font-size: 36rpx;
  color: white;
  text-align: center;
  background-color: #4963C4;
}


/* 测评提交成功模态框 */
.submitModel{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0rpx;
  z-index: 2;
  background-color: white;
}

.modelContent{
  position: relative;
  margin: auto;
  margin-top: 30%;
  width: 600rpx;
  height: 480rpx;
  text-align: center;
  font-size: 30rpx;
  color: #333;
}

.feedicon,.feedimg{
  text-align: center;
  margin: auto;
  width: 184rpx;
  height: 184rpx;
  margin-bottom: 50rpx;
}

.foottxt{
  position: absolute;
  bottom: 100rpx;
  width: 100%;
  font-size: 24rpx;
  color: #333;
  text-align: center;
}