.progress{
  display: flex;
  height: 72rpx;
  font-size: 24rpx;
  background-color: rgba(245,245,245,1);
  align-items: center;
  color: #333;
}

.progress>progress{
  margin-left: 24rpx;
  width: 569rpx;
  height: 20rpx;
  background-color:#D7DEF7;
}

.numinfo{
  margin-left: 40rpx;
}

.noworder{
  color: red;
}

.turnsubject{
  display: flex;
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  border: 2rpx solid #4963C4;
  align-items: center;
}

.primary{
  width: 50%;
  height: 96rpx;
  line-height: 96rpx;
  font-size: 36rpx;
  text-align: center;
  color: #4963C4;
}

.pagedown{
  color: white;
  background-color: #4963C4;
}

.firstpage{
  width: 100%;
  height: 96rpx;
  line-height: 96rpx;
  font-size: 36rpx;
  color: white;
  text-align: center;
  background-color: #4963C4;
}